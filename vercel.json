{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --force",
  "framework": "docusaurus",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
